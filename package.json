  "license": "Apache-2.0",
  "main": "dist/lib/index.js",
  "module": "dist/esm/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "public/MGnify-logo.svg"
  ],
  "dependencies": {
    "@fontsource/roboto": "^4.5.5",
    "@gmod/bgzf-filehandle": "^2.0.4",
    "@gmod/tabix": "^1.0.3",
    "@jbrowse/core": "^3.5.1",
    "@jbrowse/plugin-linear-genome-view": "^3.5.1",
    "@jbrowse/product-core": "^3.5.1",
    "@jbrowse/react-app2": "^3.5.1",
    "@jbrowse/sv-core": "^3.5.0",
    "@mui/material": "^5.0.0",
    "@mui/system": "^5.0.0",
    "buffer": "^6.0.3",
    "generic-filehandle": "^3.0.6",
    "pako": "^2.1.0",
    "react-draggable": "^4.4.5",
    "reselect": "^5.1.1"
  },
  "peerDependencies": {
    "@jbrowse/sv-core": "^3.5.0",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "scripts": {
    "start": "vite",
    "build:app": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "predeploy": "npm run build:app",
    "deploy": "aws s3 sync --delete build s3://jbrowse.org/demos/app-cra5/",
    "postdeploy": "AWS_PAGER=\"\" aws cloudfront  create-invalidation --distribution-id E13LGELJOT4GQO --paths \"/demos/app-cra5/*\"",
    "build": "npm run build:lib && npm run build:esm",
    "build:lib": "tsc -p tsconfig.lib.json",
    "build:esm": "tsc -p tsconfig.lib.json --module esnext --outDir dist/esm",
    "prepare": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "React components for embedding JBrowse genome viewers in MGnify (GeneViewer, JBrowseContigViewer)",
  "repository": {
    "type": "git",
    "url": "https://github.com/EBI-Metagenomics/mgnify-jbrowse.git"
  },
  "homepage": "https://github.com/EBI-Metagenomics/mgnify-jbrowse#readme",
  "bugs": {
    "url": "https://github.com/EBI-Metagenomics/mgnify-jbrowse/issues"
  },
  "keywords": [
    "jbrowse",
    "mgnify",
    "genome",
    "gene-viewer",
    "metagenomics",
    "react"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "overrides": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/node": "^20.19.0",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-jest": "^29.4.6",
    "typescript": "^4.9.5",
    "vite": "^7.3.1",
    "web-vitals": "^3.3.1"
  }
